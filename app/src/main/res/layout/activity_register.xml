<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="devep.veralezama.app.apptaxi.RegisterActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <EditText
            android:id="@+id/edtLogEmail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Email"/>
        <EditText
            android:id="@+id/edtLogNombres"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Nombres"/>
        <EditText
            android:id="@+id/edtLogApellidos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Apellidos"/>
        <EditText
            android:id="@+id/edtLogCelular"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Celular"/>
        <EditText
            android:id="@+id/edtLogPass"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="ContraseÃ±a"/>

    </LinearLayout>

</RelativeLayout>
